         </div>
      </div>   

      <div class="rightInformation">
         <div class = "videoFootage">
           <img class = "mainImage" src = "dwarveinImage.png">
         </div>
         
          <div  class = "rightContent">
            <% var skills = "";
               var keynotes = "";
            listResults.forEach(function (item) { 
               if (item.category != null){ %>
                  <div class = "rightSplit">
                     <div class = "rightItemBox">
                        <h2>
                           <%= item.category %>: <%= item.title %>
                        </h2>
                        <p>
                        <%- item.maindescription %> 
                        </p>
                     </div>
                     <div class = "contentImageBox">
                        <img class = "SquareImage" src = './<%=item.ImageSquare%>.png' alt = '<%=item.ImageSquare%>'>
                     </div>
                  </div>
               <% }}) %>
            </div>


      </div>
   </div>
    
